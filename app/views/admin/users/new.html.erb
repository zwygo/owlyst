<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
  <head>
    <script src="http://code.jquery.com/jquery-latest.min.js" type="text/javascript"></script>
    <script src="/js/ajax.js" type="text/javascript"></script>
    <script type="text/javascript">
    $(document).ready(function() {
      $("#ajax_test").click(function() {
        OA.call("user.test", {p1: "z", p2: "money"}, 1, function(data) {
          console.log(data);
        }, function(err) {
          console.log(err);
        });
      });
      $("#create_user").click(function() {
        params = {
          name: $("#user_name").val(),
          email: $("#user_email").val(),
          password: $("#password").val()
        }
        OA.call("user.create", params, 1, function(data) {
          console.log(data);
        }, function(err) {
          console.log(err);
        });
      });
    });
    </script>
  </head>
  <body>
    Name: <input type="text" id="user_name"><br>
    Email: <input type="text" id="user_email"><br>
    Password: <input type="password" id="password"><br>
    <button id="create_user">Create</button><br>
    <button id="ajax_test">Ajax Test</button><br>
  </body>
</html>